<IfModule mod_rewrite.c>
RewriteEngine On
RewriteBase /
RewriteRule ^index\.php$ - [L]
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule . /index.php [L]
</IfModule>

AuthType Basic
AuthName "Private zone"
AuthUserFile /var/www/admin/pass/.htpasswd
<Files *.php>
Require valid-user 
</Files>